import java.util.Arrays;

public class PrEj07ArraysBidi {
	static int[][] arrayBD = new int[5][10];// F C -->

	static void rellenar() {
		for (int i = 0; i < arrayBD.length; i++) {
			for (int j = 0; j < arrayBD.length; j++) {
				arrayBD[i][j] = (int) (Math.random() * 30 + 1);
				System.out.println(arrayBD[i][j]);
			}

		}

	}

	public static void main(String[] args) {
		rellenar();
		

	}

}
